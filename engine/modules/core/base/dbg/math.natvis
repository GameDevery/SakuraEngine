    <!--float matrix-->
    <Type Name="skr::math::float3x3">
        <DisplayString>({axis_x}, {axis_y}, {axis_z})</DisplayString>
        <Expand>
            <Item Name="[X]">axis_x</Item>
            <Item Name="[Y]">axis_y</Item>
            <Item Name="[Z]">axis_z</Item>
        </Expand>
    </Type>
    <Type Name="skr::math::float4x4">
        <DisplayString>({axis_x}, {axis_y}, {axis_z}, {axis_w})</DisplayString>
        <Expand>
            <Item Name="[X]">axis_x</Item>
            <Item Name="[Y]">axis_y</Item>
            <Item Name="[Z]">axis_z</Item>
            <Item Name="[W]">axis_w</Item>
        </Expand>
    </Type>
    <!--double matrix-->
    <Type Name="skr::math::double3x3">
