    <!--quat-->
    <Type Name="skr::math::QuatF">
        <DisplayString>({x}, {y}, {z}, {w})</DisplayString>
    </Type>
    <Type Name="skr::math::QuatD">
        <DisplayString>({x}, {y}, {z}, {w})</DisplayString>
    </Type>
    <!--rotator-->
    <Type Name="skr::math::RotatorF">
        <DisplayString>({pitch * 180.f / 3.14159265358979323846f}_deg, {yaw * 180.f / 3.14159265358979323846f}_deg, {roll * 180.f / 3.14159265358979323846f}_deg)</DisplayString>
        <Expand>
            <Item Name="[DEG] pitch">pitch * 180.f / 3.14159265358979323846f</Item>
            <Item Name="[DEG] yaw">yaw * 180.f / 3.14159265358979323846f</Item>
            <Item Name="[DEG] roll">roll * 180.f / 3.14159265358979323846f</Item>

            <Item Name="[RAD] pitch">pitch</Item>
            <Item Name="[RAD] yaw">yaw</Item>
            <Item Name="[RAD] roll">roll</Item>
        </Expand>
    </Type>
    <Type Name="skr::math::RotatorD">
        <DisplayString>({x * 180.f / 3.14159265358979323846f}_deg, {yaw * 180.f / 3.14159265358979323846f}_deg, {roll * 180.f / 3.14159265358979323846f}_deg)</DisplayString>
        <Expand>
            <Item Name="[DEG] pitch">pitch * 180.f / 3.14159265358979323846f</Item>
            <Item Name="[DEG] yaw">yaw * 180.f / 3.14159265358979323846f</Item>
            <Item Name="[DEG] roll">roll * 180.f / 3.14159265358979323846f</Item>

            <Item Name="[RAD] pitch">pitch</Item>
            <Item Name="[RAD] yaw">yaw</Item>
            <Item Name="[RAD] roll">roll</Item>
        </Expand>
    </Type>
</AutoVisualizer>