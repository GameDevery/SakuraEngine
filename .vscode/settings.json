{
    "dascript.experimental": true,
    "dascript.server.connectTimeout": 10,
    "Lua.diagnostics.disable": [
        "undefined-global",
        "lowercase-global",
        "undefined-field",
        "need-check-nil"
    ],
    "cmake.ignoreCMakeListsMissing": true,
    "files.associations": {
        "*.mm": "objective-cpp",
        "*.m": "objective-c",
        "*.hppsl": "cpp",
        "*.cppsl": "cpp"
    }
}